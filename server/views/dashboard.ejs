<%- include('header'); -%>
<div class="container">
    <div class="row">
        <div class="col-md-3 col-sm-3 col-lg-3"></div>
        <div class="col-md-6 col-sm-6 col-lg-6"> 
            <h2 class="table-title">Key Performance Indicators</h2>    
            <table id="table" data-toggle="table" data-search="false" class="table-bordered">
                <thead class="thead-dark">
                <tr>
                    <th scope="col">Name</th>
                    <th scope="col">Count</th>
                </tr>
                </thead>
                <tbody class="table-light"> 
                    <% for (var i = 0; i < res.length; i++) {%>
                        <tr>
                        <td><%=res[i].name%></td>
                        <td><%=res[i].count%></td>
                        </tr>
                    <%};%>
                </tbody>
            </table>
        </div>
        <div class="col-md-3 col-sm-3 col-lg-3"></div>
    </div>   
</div>
